{
  "name": "raven-client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/raven-client/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/raven-client",
        "index": "apps/raven-client/src/index.html",
        "main": "apps/raven-client/src/main.ts",
        "polyfills": "apps/raven-client/src/polyfills.ts",
        "tsConfig": "apps/raven-client/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/raven-client/src/favicon.ico",
          "apps/raven-client/src/assets"
        ],
        "styles": [
          "node_modules/@progress/kendo-theme-default/dist/all.css",
          "apps/raven-client/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "3mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "25kb",
              "maximumError": "50kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/raven-client/src/environments/environment.ts",
              "with": "apps/raven-client/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false
        },
        "development": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "3mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "25kb",
              "maximumError": "50kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/raven-client/src/environments/environment.ts",
              "with": "apps/raven-client/src/environments/environment.development.ts"
            }
          ],
          "outputHashing": "all",
          "sourceMap": true,
          "namedChunks": true
        },
        "local": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "raven-client:build:production"
        },
        "development": {
          "browserTarget": "raven-client:build:development"
        },
        "local": {
          "browserTarget": "raven-client:build:local"
        }
      },
      "defaultConfiguration": "local"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "raven-client:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/raven-client/**/*.ts",
          "apps/raven-client/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/raven-client"],
      "options": {
        "jestConfig": "apps/raven-client/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 6006,
        "configDir": "apps/raven-client/.storybook",
        "browserTarget": "raven-client:build",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/raven-client",
        "configDir": "apps/raven-client/.storybook",
        "browserTarget": "raven-client:build",
        "compodoc": false,

        "styles": [
          "node_modules/@progress/kendo-theme-default/dist/all.css",

          "apps/raven-client/.storybook/styles.scss"
        ]
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": []
}
